test_getter_repr:
  -
    getter: byoc.Key(byoc.Config)
    expected: byoc.Key\(Config\)
  -
    getter: byoc.Key(byoc.Config, 'x')
    expected: byoc.Key\(Config, 'x'\)
  -
    getter: byoc.Key(byoc.Config, cast=int)
    expected: byoc.Key\(Config, cast=<class 'int'>\)
  -
    getter: byoc.Key(byoc.Config, toggle=True)
    expected: byoc.Key\(Config, toggle=True\)
  -
    getter: byoc.Method(lambda self: 1)
    expected: byoc.Method\(<function <lambda> at 0x.*>\)
  -
    getter: byoc.Func(lambda: 1)
    expected: byoc.Func\(<function <lambda> at 0x.*>\)
  -
    getter: byoc.Value(1)
    expected: byoc.Value\(1\)

test_getter_cast_value:
  -
    id: value
    getter: byoc.Value(None)
    given: 1
    expected: 1
  -
    id: value
    getter: byoc.Value(None)
    param: byoc.param(cast=int)
    given: '1'
    expected: 1
  -
    id: value-err
    getter: byoc.Value(None)
    param: byoc.param(cast=int)
    given: 'a'
    error:
      type: ValueError
      message:
        - invalid literal for int() with base 10: 'a'

  -
    id: func
    getter: byoc.Func(lambda: None)
    given: 1
    expected: 1
  -
    id: func
    getter: byoc.Func(lambda: None)
    param: byoc.param(cast=int)
    given: '1'
    expected: 1
  -
    id: func-err
    getter: byoc.Func(lambda: None)
    param: byoc.param(cast=int)
    given: 'a'
    error:
      type: ValueError
      message:
        - invalid literal for int() with base 10: 'a'

  -
    id: method
    getter: byoc.Method(lambda self: None)
    given: 1
    expected: 1
  -
    id: method
    getter: byoc.Method(lambda self: None)
    param: byoc.param(cast=int)
    given: '1'
    expected: 1
  -
    id: method-err
    getter: byoc.Method(lambda self: None)
    param: byoc.param(cast=int)
    given: 'a'
    error:
      type: ValueError
      message:
        - invalid literal for int() with base 10: 'a'

  -
    id: key
    getter: byoc.Key(byoc.Config)
    given: 1
    expected: 1
  -
    id: key
    getter: byoc.Key(byoc.Config, cast=int)
    given: '1'
    expected: 1
  -
    id: key
    getter: byoc.Key(byoc.Config)
    param: byoc.param(cast=int)
    given: '1'
    expected: 1
  -
    id: key
    getter: byoc.Key(byoc.Config, cast=int)
    param: byoc.param(cast=lambda x: x+1)
    given: '1'
    expected: 2
  -
    id: key-err
    getter: byoc.Key(byoc.Config, cast=int)
    given: 'a'
    error:
      type: ValueError
      message:
        - invalid literal for int() with base 10: 'a'
