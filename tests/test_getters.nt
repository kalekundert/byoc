test_getter_iter_values:
  -
    id: value
    getter: byoc.Value(1)
    expected:
      values: [1]
      meta:
        -
          type: GetterMeta
          getter: Value
          getter.value: 1
      dynamic:
        - False
      log:
        - got hard-coded value: 1

  -
    id: func
    getter: byoc.Func(lambda: 1)
    expected:
      values: [1]
      meta:
        -
          type: GetterMeta
          getter: Func
      dynamic:
        - False
      log:
        -
          > called: <function <lambda> at .*>
          > returned: 1
  -
    id: func-partial
    getter: byoc.Func(lambda x: x + 1).partial(1)
    expected:
      values: [2]
      meta:
        -
          type: GetterMeta
          getter: Func
      dynamic:
        - False
      log:
        -
          > called: <function <lambda> at .*>
          > returned: 2
  -
    id: func-dynamic
    getter: byoc.Func(lambda: 1, dynamic=True)
    expected:
      values: [1]
      meta:
        -
          type: GetterMeta
          getter: Func
      dynamic:
        - True
      log:
        -
          > called: <function <lambda> at .*>
          > returned: 1
  -
    id: func-err
    getter: byoc.Func(lambda: 1/0)
    error:
      type: ZeroDivisionError
  -
    id: func-err-skip
    getter: byoc.Func(lambda: 1/0, skip=ZeroDivisionError)
    expected:
      values: []
      meta:
      dynamic:
      log:
        -
          > called: <function <lambda> at .*>
          > raised ZeroDivisionError: division by zero

  -
    id: method
    obj:
      > class DummyObj:
      >     __config__ = []
      >     x = 1
      >
    getter: byoc.Method(lambda self: self.x)
    expected:
      values: [1]
      meta:
        -
          type: GetterMeta
          getter: Method
      dynamic:
        - True
      log:
        -
          > called: <function <lambda> at .*>
          > returned: 1
  -
    id: method
    obj:
      > class DummyObj:
      >     __config__ = []
      >     x = 1
      >
      >     def helper(self):
      >         return self.x + 1
      >
    getter: byoc.Method(DummyObj.helper)
    expected:
      values: [2]
      meta:
        -
          type: GetterMeta
          getter: Method
      dynamic:
        - True
      log:
        -
          > called: <function DummyObj.helper at .*>
          > returned: 2
  -
    id: method
    obj:
      > class DummyObj:
      >     __config__ = []
      >     x = 1
      >
    getter: byoc.Method(lambda self, y: self.x + y).partial(1)
    expected:
      values: [2]
      meta:
        -
          type: GetterMeta
          getter: Method
      dynamic:
        - True
      log:
        -
          > called: <function <lambda> at .*>
          > returned: 2
  -
    id: method-dynamic
    obj:
      > class DummyObj:
      >     __config__ = []
      >     x = 1
      >
    getter: byoc.Method(lambda self: self.x, dynamic=False)
    expected:
      values: [1]
      meta:
        -
          type: GetterMeta
          getter: Method
      dynamic:
        - False
      log:
        -
          > called: <function <lambda> at .*>
          > returned: 1
  -
    id: method-err
    obj:
      > class DummyObj:
      >     __config__ = []
      >
    getter: byoc.Method(lambda self: 1/0)
    error:
      type: ZeroDivisionError
  -
    id: method-err-skip
    obj:
      > class DummyObj:
      >     __config__ = []
      >
    getter: byoc.Method(lambda self: 1/0, skip=ZeroDivisionError)
    expected:
      values: []
